{
    "family": "backend-flask",
    "executionRoleArn": "arn:aws:iam::785139023983:role/CruddurServiceExecutionRole",
    "taskRoleArn": "arn:aws:iam::785139023983:role/CruddurTaskRole",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilites": [
      "FARGATE"
    ],
    "containerDefinitions": [
      {
        "name": "backend-flask",
        "image": "785139023983.dkr.ecr.us-east-2.amazonaws.com/backend-flask",
        "essential": true,
        "healthCheck": {
          "command": [
            "CMD-SHELL",
            "python /backend-flask/bin/flask/health-check"
          ],
          "interval": 30,
          "timeout": 5,
          "retries": 3,
          "startPeriod": 60
        },
        "portMappings": [
          {
            "name": "backend-flask",
            "containerPort": 4567,
            "protocol": "tcp",
            "appProtocol": "http"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "cruddur/fargate-cluster",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "backend-flask"
          }
        },
        "environment": [
          {"name": "OTEL_SERVICE_NAME", "value": "backend-flask"},
          {"name": "OTEL_EXPORTER_OTLP_ENDPOINT", "value": "https://api.honeycomb.io"},
          {"name": "AWS_COGNITO_USER_POOL_ID", "value": "us-east-2_7cm4Q6fHv"},
          {"name": "AWS_COGNITO_USER_POOL_CLIENT_ID", "value": "50tfbjvr17fs6u7ontrgiai5sa"},
          {"name": "FRONTEND_URL", "value": "*"},
          {"name": "BACKEND_URL", "value": "*"},
          {"name": "AWS_DEFAULT_REGION", "value": "us-east-2"}
        ],
        "secrets": [
          {"name": "AWS_ACCESS_KEY_ID", "valueFrom": "arn:ssm:us-east-2:785139023983:parameter/cruddur/backend-flask/AWS_ACCESS_KEY_ID"},
          {"name": "AWS_SECRET_ACCESS_KEY", "valueFrom": "arn:ssm:us-east-2:785139023983:parameter/cruddur/backend-flask/AWS_SECRET_ACCESS_KEY"},
          {"name": "CONNECTION_URL", "valueFrom": "arn:ssm:us-east-2:785139023983:parameter/cruddur/backend-flask/CONNECTION_URL"},
          {"name": "ROLLBAR_ACCESS_TOKEN", "valueFrom": "arn:ssm:us-east-2:785139023983:parameter/cruddur/backend-flask/ROLLBAR_ACCESS_TOKEN"},
          {"name": "OTEL_EXPORTER_OTLP_HEADERS", "valueFrom": "x-honeycomb-team=${HONEYCOMB_API_KEY}"}
        ]
      }
    ]
  }